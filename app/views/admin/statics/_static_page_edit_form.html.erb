<!-- Form to update title and subtitle -->
<%= semantic_form_for [:admin, resource], :remote => true, builder: ActiveAdmin::FormBuilder do |f| %>
   <%=  f.inputs do %>
     <%= f.input :title %>
     <%= f.input :subtitle %>
  <% end %>
  <%= f.actions do %>
    <%= f.action :submit, :label => "Update Heading"%>
  <% end %>
<% end %>

<!-- Form to update website content -->
<%= render 'static_page_content_editor', page: Static.find_by_id(params[:id]) %>